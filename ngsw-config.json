{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "app",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/*.css",
          "/*.js",
          "/manifest.webmanifest"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**"
        ]
      }
    },
    {
      "name": "google-resources",
      "installMode": "lazy",
      "updateMode": "lazy",
      "resources": {
        "urls": [
          "https://www.google-analytics.com/**",
          "https://www.googletagmanager.com/**",
          "https://fonts.googleapis.com/**",
          "https://fonts.gstatic.com/**"
        ]
      }
    },
    {
      "name": "cloudflare-resources",
      "installMode": "lazy",
      "updateMode": "lazy",
      "resources": {
        "urls": [
          "https://static.cloudflareinsights.com/**"
        ]
      }
    }
  ],
  "dataGroups": [
    {
      "name": "youtube-thumbnails",
      "urls": [
        "https://img.youtube.com/vi/**",
        "https://*.ytimg.com/**",
        "https://*.googlevideo.com/**"
      ],
      "cacheConfig": {
        "strategy": "freshness",
        "maxSize": 100,
        "maxAge": "7d",
        "timeout": "10s"
      }
    },
    {
      "name": "api-cache",
      "urls": [
        "/api/**"
      ],
      "cacheConfig": {
        "strategy": "performance",
        "maxSize": 100,
        "maxAge": "1d",
        "timeout": "5s"
      }
    }
  ]
}